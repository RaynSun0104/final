    "\n",
    "\n",
    "features = pd.read_csv(\"clean_outputs/neighborhood_features.csv\")\n",
    "\n",
    "\n",
    "features[\"risk_freq\"] = features[\"arrests_per_month\"]\n",
    "features[\"risk_felony\"] = features[\"felony_ratio\"]\n",
    "features[\"risk_misdemeanor\"] = features[\"misdemeanor_ratio\"]\n",
    "features[\"risk_infraction\"] = features[\"infraction_ratio\"]\n",
    "features[\"risk_violence\"] = features[\"violent_ratio\"]\n",
    "features[\"risk_property\"] = features[\"property_ratio\"]\n",
    "features[\"risk_society\"]  = features[\"society_ratio\"]\n",
    "features[\"risk_night\"] = features[\"night_ratio\"]\n",
    "\n",
    "\n",
    "features[\"risk_trend_raw\"] = features[\"yoy_change\"].clip(lower=0)  # <0 marked as 0\n",
    "features[\"risk_trend\"] = features[\"risk_trend_raw\"]\n",
    "\n",
    "\n",
    "\n",
    "def min_max_norm(series):\n",
    "    s = series.astype(float)\n",
    "    s = s.replace([np.inf, -np.inf], np.nan)\n",
    "    s_min = s.min()\n",
    "    s_max = s.max()\n",
    "    if pd.isna(s_min) or pd.isna(s_max) or s_max == s_min:\n",
    "        return pd.Series(0.0, index=series.index)\n",
    "    return (s - s_min) / (s_max - s_min)\n",
    "\n",
    "features[\"risk_freq_norm\"] = min_max_norm(features[\"risk_freq\"])\n",
    "features[\"risk_felony_norm\"] = min_max_norm(features[\"risk_felony\"])\n",
    "features[\"risk_misdemeanor_norm\"] = min_max_norm(features[\"risk_misdemeanor\"])\n",
    "features[\"risk_infraction_norm\"] = min_max_norm(features[\"risk_infraction\"])\n",
    "features[\"risk_violence_norm\"] = min_max_norm(features[\"risk_violence\"])\n",
    "features[\"risk_property_norm\"] = min_max_norm(features[\"risk_property\"])\n",
    "features[\"risk_society_norm\"] = min_max_norm(features[\"risk_society\"])\n",
    "features[\"risk_night_norm\"] = min_max_norm(features[\"risk_night\"])\n",
    "features[\"risk_trend_norm\"] = min_max_norm(features[\"risk_trend\"])\n",
    "\n",
    "\n",
    "\n",
    "w_freq = 0.20\n",
    "w_felony = 0.20\n",
    "w_misdemeanor = 0.10\n",
    "w_infraction = 0.05\n",
    "w_violence = 0.15\n",
    "w_property = 0.10\n",
    "w_society = 0.05\n",
    "w_night = 0.10\n",
    "w_trend = 0.05\n",
    "\n",
    "features[\"Risk_Index\"] = (\n",
    "    w_freq      * features[\"risk_freq_norm\"] +\n",
    "    w_felony  * features[\"risk_felony_norm\"] +\n",
    "    w_misdemeanor * features[\"risk_misdemeanor_norm\"] +\n",
    "    w_infraction * features[\"risk_infraction_norm\"] +\n",
    "    w_violence  * features[\"risk_violence_norm\"] +\n",
    "    w_property  * features[\"risk_property_norm\"] +\n",
    "    w_society   * features[\"risk_society_norm\"] +\n",
    "    w_night     * features[\"risk_night_norm\"] +\n",
    "    w_trend     * features[\"risk_trend_norm\"]\n",
    ")\n",
    "\n",
    "\n",
    "\n",
    "features[\"Safety_Score\"] = (1 - features[\"Risk_Index\"]) \n",
    "features[\"Safety_Score\"] = features[\"Safety_Score\"].clip(lower=0, upper=100)\n",
    "\n",
    "\n",
    "features_sorted = features.sort_values(\"Safety_Score\", ascending=False)\n",
    "\n",
    "\n",
    "output_cols = [\n",
    "    \"Safety_Score\",\n",
    "]\n",
    "output_cols = [c for c in output_cols if c in features_sorted.columns]\n",
    "\n",
    "\n",
    "features_sorted_reset = features_sorted.reset_index()  \n",
    "output_path = \"clean_outputs/neighborhood_scores.csv\"\n",
    "features_sorted_reset[[\"Neighborhood\"] + output_cols].to_csv(output_path, index=False)\n",
    "\n",
    "\n",
    "print(\"Scores is now in :\", output_path)\n",
    "print(\"Top 10 neighborhoods:\")\n",
    "print(features_sorted_reset[[\"Neighborhood\", \"Safety_Score\"]].head(10))\n",
    "\n",
    "\n",
    "if \"Neighborhood\" not in features.columns:\n",
    "    features = features.reset_index()\n",
    "\n",
    "\n",
    "SCORE_COL = \"Risk_Index\"\n",
    "top10_safe = features.sort_values(SCORE_COL, ascending=True).head(10)\n",
