    "\n",
    "\n",
    "df = pd.read_csv(\"clean_outputs/pbp_arrests_cleaned.csv\")\n",
    "\n",
    "\n",
    "df[\"Arrest_Time\"] = pd.to_datetime(\n",
    "    df[\"Arrest_Time\"],\n",
    "    format=\"%H:%M:%S\",\n",
    "    errors=\"coerce\"\n",
    ").dt.time\n",
    "\n",
    "def is_night(t):\n",
    "    if isinstance(t, dt.time):\n",
    "        h = t.hour\n",
    "    elif isinstance(t, (pd.Timestamp, dt.datetime)):\n",
    "        h = t.hour\n",
    "    else:\n",
    "        \n",
    "        s = str(t).strip()\n",
    "        \n",
    "        parsed = pd.to_datetime(s, format=\"%H:%M:%S\", errors=\"coerce\")\n",
    "        if pd.isna(parsed):\n",
    "            \n",
    "            parsed = pd.to_datetime(s, errors=\"coerce\")\n",
    "        if pd.isna(parsed):\n",
    "            return 0  \n",
    "        h = parsed.hour\n",
    "\n",
    "    return 1 if (h >= 22 or h < 5) else 0\n",
    "\n",
    "df[\"Is_Night\"] = df[\"Arrest_Time\"].apply(is_night)\n",
    "\n",
    "\n",
    "def charge_type(x):\n",
    "    if pd.isna(x):\n",
    "        return \"UNKNOWN\"\n",
    "    if \"FELONY\" in x:\n",
    "        return \"FELONY\"\n",
    "    if \"MISDEMEANOR\" in x:\n",
    "        return \"MISDEMEANOR\"\n",
    "    if \"INFRACTION\" in x:\n",
    "        return \"INFRACTION\"\n",
    "    return \"UNKNOWN\"\n",
    "\n",
    "df[\"Charge_Type\"] = df[\"ArrestCharge_Felony_Misdemeanor_Description\"].apply(charge_type)\n",
    "\n",
    "\n",
    "\n",
    "group = df.groupby(\"Neighborhood\")\n",
    "\n",
    "# --- toal arrests ---\n",
    "features = pd.DataFrame()\n",
    "features[\"total_arrests\"] = group.size()\n",
    "\n",
    "# --- arrests per month ---\n",
    "month_count = df.groupby([\"Neighborhood\", \"Arrest_Year\", \"Arrest_Month\"]).size().groupby(\"Neighborhood\").mean()\n",
    "features[\"arrests_per_month\"] = month_count\n",
    "\n",
    "# --- severity ---\n",
    "features[\"felony_count\"] = group[\"Charge_Type\"].agg(lambda s: (s == \"FELONY\").sum())\n",
    "features[\"misdemeanor_count\"] = group[\"Charge_Type\"].agg(lambda s: (s == \"MISDEMEANOR\").sum())\n",
    "features[\"infraction_count\"] = group[\"Charge_Type\"].agg(lambda s: (s == \"INFRACTION\").sum())\n",
    "\n",
    "features[\"felony_ratio\"] = features[\"felony_count\"] / features[\"total_arrests\"]\n",
    "features[\"misdemeanor_ratio\"] = features[\"misdemeanor_count\"] / features[\"total_arrests\"]\n",
    "features[\"infraction_ratio\"] = features[\"infraction_count\"] / features[\"total_arrests\"]\n",
    "\n",
    "# --- categories ---\n",
    "\n",
    "df[\"CrimeGroup\"] = df[\"NIBRS_Crime_Against\"].str.upper().str.strip()\n",
    "\n",
    "df[\"CrimeGroup\"] = df[\"CrimeGroup\"].replace({\n",
    "    \"PERSON, PROPERTY, OR SOCIETY\": \"MULTI\"\n",
    "})\n",
    "\n",
    "group = df.groupby(\"Neighborhood\")\n",
    "\n",
    "features[\"violent_crime_count\"] = group.apply(lambda x: (x[\"CrimeGroup\"]==\"PERSON\").sum())\n",
    "features[\"property_crime_count\"] = group.apply(lambda x: (x[\"CrimeGroup\"]==\"PROPERTY\").sum())\n",
    "features[\"society_crime_count\"] = group.apply(lambda x: (x[\"CrimeGroup\"]==\"SOCIETY\").sum())\n",
    "\n",
    "features[\"groupB_crime_count\"] = group.apply(lambda x: (x[\"CrimeGroup\"]==\"GROUP B\").sum())\n",
    "\n",
    "features[\"multi_crime_count\"] = group.apply(lambda x: (x[\"CrimeGroup\"]==\"MULTI\").sum())\n",
    "\n",
    "features[\"violent_ratio\"] = features[\"violent_crime_count\"] / features[\"total_arrests\"]\n",
    "features[\"property_ratio\"] = features[\"property_crime_count\"] / features[\"total_arrests\"]\n",
    "features[\"society_ratio\"] = features[\"society_crime_count\"] / features[\"total_arrests\"]\n",
    "features[\"groupB_ratio\"] = features[\"groupB_crime_count\"] / features[\"total_arrests\"]\n",
    "\n",
    "\n",
    "# --- night crime ---\n",
    "features[\"night_crime_count\"] = group[\"Is_Night\"].sum()\n",
    "features[\"night_ratio\"] = features[\"night_crime_count\"] / features[\"total_arrests\"]\n",
    "\n",
    "\n",
    "\n",
    "year_group = df.groupby([\"Neighborhood\", \"Arrest_Year\"]).size().unstack(fill_value=0)\n",
    "\n",
    "for yr in [2024, 2025]:\n",
    "    if yr not in year_group:\n",
    "        year_group[yr] = 0\n",
    "\n",
    "features[\"arrests_2024\"] = year_group.get(2024, 0)\n",
    "features[\"arrests_2025\"] = year_group.get(2025, 0)\n",
    "features[\"yoy_change\"] = (features[\"arrests_2025\"] - features[\"arrests_2024\"]) / (features[\"arrests_2024\"] + 1)\n",
    "\n",
    "# decrease more than 10%\n",
    "features[\"improving_flag\"] = (features[\"yoy_change\"] < -0.1).astype(int)\n",
    "\n",
    "\n",
    "\n",
    "features.fillna(0, inplace=True)\n",
    "\n",
    "output_path = \"clean_outputs/neighborhood_features.csv\"\n",
    "features.to_csv(output_path)\n",
    "\n",
    "\n",
    "print(\"Feature : \", output_path)\n",
    "print(\"First 5 rows\")\n",
    "print(features.head())\n",
    "\n",
    "\n",
    "\n"
   ]
