    "import os\n",
    "import re\n",
    "import datetime as dt\n",
    "import numpy as np\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "pd.set_option(\"display.max_columns\", None)\n",
    "pd.set_option(\"display.width\", 200)\n",
    "\n",
    "\n",
    "raw_file_path = \"pbp_arrests_2024_jan1_thru_oct31_2025.xlsx\"\n",
    "\n",
    "df = pd.read_excel(raw_file_path)\n",
    "\n",
    "print(f\"The dataset has :{df.shape[0]} rows x {df.shape[1]} columns\")\n",
    "print()\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "if \"Neighborhood\" in df.columns:\n",
    "    before = df.shape[0]\n",
    "    df = df[~df[\"Neighborhood\"].isna()]\n",
    "    \n",
    "\n",
    "\n",
    "if \"Arrest_Date\" in df.columns:\n",
    "    before = df.shape[0]\n",
    "    df = df[~df[\"Arrest_Date\"].isna()]\n",
    "    \n",
    "\n",
    "\n",
    "\n",
    "text_cols = [\n",
    "    \"Template\",\n",
    "    \"Type\",\n",
    "    \"Neighborhood\",\n",
    "    \"NIBRS_Offense_Category\",\n",
    "    \"NIBRS_Crime_Against\",\n",
    "    \"NIBRS_Offense_Grouping\",\n",
    "    \"ArrestCharge_Felony_Misdemeanor_Description\",\n",
    "]\n",
    "\n",
    "for col in text_cols:\n",
    "    if col in df.columns:\n",
    "        df[col] = df[col].astype(str).str.upper().str.strip()\n",
    "\n",
    "\n",
    "def normalize_charge_label(val):\n",
    "    \n",
    "    if pd.isna(val):\n",
    "        return np.nan\n",
    "\n",
    "    s = str(val).upper().strip()\n",
    "\n",
    "    \n",
    "    s = s.replace(\"--\", \"-\")\n",
    "    s = \" \".join(s.split())  \n",
    "\n",
    "    \n",
    "    if \"FEL\" in s:\n",
    "        base = \"FELONY\"\n",
    "    elif \"MISD\" in s or \"MISDEMEANOR\" in s:\n",
    "        base = \"MISDEMEANOR\"\n",
    "    elif \"INFRACTION\" in s or \"SUMMARY\" in s:\n",
    "        base = \"INFRACTION\"\n",
    "    else:\n",
    "        return \"UNKNOWN\"\n",
    "\n",
    "    \n",
    "    match = re.search(r\"(F\\d|M\\d|S)$\", s)\n",
    "    if match:\n",
    "        suffix = match.group(1)\n",
    "        return f\"{base} - {suffix}\"\n",
    "\n",
    "    return base\n",
    "\n",
    "\n",
    "if \"ArrestCharge_Felony_Misdemeanor_Description\" in df.columns:\n",
    "    df[\"ArrestCharge_Felony_Misdemeanor_Description\"] = (\n",
    "        df[\"ArrestCharge_Felony_Misdemeanor_Description\"].apply(normalize_charge_label)\n",
    "    )\n",
    "\n",
    "\n",
    "\n",
    "if \"Arrest_Date\" in df.columns:\n",
    "    df[\"Arrest_Date\"] = pd.to_datetime(df[\"Arrest_Date\"], errors=\"coerce\")\n",
    "\n",
    "    \n",
    "    before = df.shape[0]\n",
    "    df = df[~df[\"Arrest_Date\"].isna()]\n",
    "\n",
    "    df[\"Arrest_Year\"] = df[\"Arrest_Date\"].dt.year\n",
    "    df[\"Arrest_Month\"] = df[\"Arrest_Date\"].dt.month\n",
    "    df[\"Arrest_Weekday\"] = df[\"Arrest_Date\"].dt.day_name()\n",
    "\n",
    "\n",
    "def parse_time_to_time(x):\n",
    "    \n",
    "    if pd.isna(x):\n",
    "        return np.nan\n",
    "\n",
    "    s = str(x).strip()\n",
    "    if s == \"\":\n",
    "        return np.nan\n",
    "\n",
    "    \n",
    "    t = pd.to_datetime(s, format=\"%H:%M\", errors=\"coerce\")\n",
    "    if pd.notna(t):\n",
    "        return t.time()\n",
    "\n",
    "\n",
    "    return np.nan\n",
    "\n",
    "\n",
    "if \"Arrest_Time\" in df.columns:\n",
    "    df[\"Arrest_Time\"] = df[\"Arrest_Time\"].apply(parse_time_to_time)\n",
    "    \n",
    "before = df.shape[0]\n",
    "df = df.drop_duplicates()\n",
    "\n",
    "cols_to_keep = [\n",
    "    \"Case_Number\",\n",
    "    \"Arrest_Date\",\n",
    "    \"Arrest_Year\",\n",
    "    \"Arrest_Month\",\n",
    "    \"Arrest_Weekday\",\n",
    "    \"Arrest_Time\",\n",
    "    \"Neighborhood\",\n",
    "    \"NIBRS_Offense_Category\",\n",
    "    \"NIBRS_Crime_Against\",\n",
    "    \"NIBRS_Offense_Grouping\",\n",
    "    \"ArrestCharge_Felony_Misdemeanor_Description\",\n",
    "    \"Template\",\n",
    "    \"Type\",\n",
    "    \"Zone\",\n",
    "    \"Tract\",\n",
    "]\n",
    "\n",
    "\n",
    "cols_to_keep = [c for c in cols_to_keep if c in df.columns]\n",
    "df = df[cols_to_keep]\n",
    "\n",
    "\n",
    "os.makedirs(\"clean_outputs\", exist_ok=True)\n",
    "clean_file_path = os.path.join(\"clean_outputs\", \"pbp_arrests_cleaned.csv\")\n",
    "\n",
    "df.to_csv(clean_file_path, index=False)\n",
    "\n",
    "print(f\"The clean dataset is now : {clean_file_path}\")\n",
    "print(f\"The clean dataset has : {df.shape[0]} rows x {df.shape[1]} columns\")\n",
    "\n",
    "\n"
   ]
